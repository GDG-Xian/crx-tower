{
	"manifest_version": 2,
	
	"name": "Twoer.im Plus",
	"description": "Improve twoer.im",
	"version": "1.0",
	
	"icons": {
		"48": "icon48.png",
		"64": "icon64.png",
		"128": "icon128.png"
	},
	
	"permissions": [
		"tabs", "https://tower.im/*"
	],
	
	"content_scripts": [
		{
			"matches": ["https://tower.im/*"],
			"js": ["jquery-2.1.0.min.js", "contentscript.js"]
		}
	]
}